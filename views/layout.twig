{% block header %}
<!DOCTYPE html>
<html {{ fn('language_attributes') }}>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        {{ fn('wp_head') }}

        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto">

        <title>{{ fn('wp_title', '&raquo;', true, 'right') }} {{ fn('get_bloginfo') }}</title>
    </head>
    <body>
        <div class="main-container">
            {{ fn('wp_nav_menu') }}
{% endblock %}

{% block container %}
<div class="content">
    {% block content %}

    {% endblock %}
</div>
{% endblock %}

{% block footer %}
            {{ fn('wp_footer') }}
        </div>
    </body>
</html>
{% endblock %}